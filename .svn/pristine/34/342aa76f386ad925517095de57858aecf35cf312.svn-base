package com.ideepmind.joy.sdk.diet;

/**
 * 筛选条件（功效，口味，人群）的详情
 * @author jixuguo
 *
 */
public class CategoryDetail extends Category {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private String dietPrinciple="";
	private String suitableFood="";
	private String tabooFood="";
	private String cookSuggestion="";
	private String preventSuggestion="";
	/** 
	 * 获取饮食要点
	 * @return 饮食要点
	 */
	public String getDietPrinciple() {
		return dietPrinciple;
	}

	/** 
	 * 赋值饮食要点
	 * @param String 饮食原则
	 */
	public void setDietPrinciple(String dietPrinciple) {
		this.dietPrinciple = dietPrinciple;
	}
	
	/** 
	 * 获取宜吃食物
	 * @return 宜吃食物
	 */
	public String getSuitableFood() {
		return suitableFood;
	}

	/** 
	 * 赋值宜吃食物
	 * @param String 宜吃食物
	 */
	public void setSuitableFood(String suitableFood) {
		this.suitableFood = suitableFood;
	}
	
	/** 
	 * 获取禁忌食物
	 * @return 禁忌食物
	 */
	public String getTabooFood() {
		return tabooFood;
	}

	/** 
	 * 赋值禁忌食物
	 * @param String 禁忌食物
	 */
	public void setTabooFood(String tabooFood) {
		this.tabooFood = tabooFood;
	}
	
	/** 
	 * 获取烹饪建议
	 * @return 烹饪建议
	 */
	public String getCookSuggestion() {
		return cookSuggestion;
	}

	/** 
	 * 赋值烹饪建议
	 * @param String 烹饪建议
	 */
	public void setCookSuggestion(String cookSuggestion) {
		this.cookSuggestion = cookSuggestion;
	}
	
	/** 
	 * 获取预防建议
	 * @return 预防建议
	 */
	public String getPreventSuggestion() {
		return preventSuggestion;
	}

	/** 
	 * 赋值预防建议
	 * @param String 预防建议
	 */
	public void setPreventSuggestion(String preventSuggestion) {
		this.preventSuggestion = preventSuggestion;
	}
	
	
	
}
